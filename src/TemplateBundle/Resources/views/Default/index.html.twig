<script>


    $(function () {
        $(document).on('click', '[data-copy-form]', function () {
            cloneForm($(this));
            return false;
        });
        $(document).on('click', '[data-delete-form]', function () {
            deleteForm($(this));
            return false;
        });
        $(document).on('click', '.confirm', function () {
            return confirm('Etes vous certain(e) de vouloir continuer cette action ? ');
        });
    });

    function deleteForm(t) {
        if (confirm('Vous allez supprimer un ligne est le contenu associé, voulez vous continuer ?')) {
            t.closest($(t).data('closest')).remove();
        }
    }


    function cloneForm(t) {
        let $collectionHolder = $(t.data('copy-form'));
        $collectionHolder.data('index', $collectionHolder.find(':input').length);
        let prototype = $collectionHolder.data('prototype');
        // get the new index
        let index = parseInt($collectionHolder.data('index'));

        // index = index  / parseInt($(prototype).find('td').length)
        let newForm = prototype;
        console.log(index, $(prototype).find('td').length);
        newForm = newForm.replace(/__name__/g, index);
        // increase the index with one for the next item
        $collectionHolder.data('index', index + 1);
        if (t.data('after') !== undefined) {
            $collectionHolder.find(t.data('after')).after(newForm);
        } else if (t.data('before') !== undefined) {
            $collectionHolder.find(t.data('before')).before(newForm);

        }
    }
</script>
